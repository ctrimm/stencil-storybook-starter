{
  "name": "stencil-storybook-starter",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-storybook-starter/stencil-storybook-starter.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "watch": "stencil build --docs --watch",
    "storybook.run": "start-storybook --port 9001",
    "storybook": "npm-run-all --parallel watch storybook.run",
    "create-component": "node ./scripts/generator-component.js"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@stencil/core": "^1.8.8",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/html": "^5.3.13",
    "@types/jest": "^24.9.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__html": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "case": "^1.6.2",
    "chalk": "^2.4.1",
    "figlet": "^1.2.0",
    "glob": "^7.1.6",
    "inquirer": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.2",
    "storybook": "^1.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.35.3"
  },
  "license": "MIT"
}
